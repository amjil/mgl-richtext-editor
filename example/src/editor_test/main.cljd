(ns editor-test.main
  (:require
   ["package:flutter/material.dart" :as m]
   [cljd.flutter :as f]
   [rich-editor.ui.editor-main :refer [mongol-editor-view]]))

(defn main []
  (m.WidgetsFlutterBinding/ensureInitialized)

  (f/run
   (f/widget
    (m/MaterialApp
     .title "MGL Rich Text Editor Test"
     .debugShowCheckedModeBanner false
     .theme (m/ThemeData
             .brightness m.Brightness/dark
             .useMaterial3 true)
     .home (m/Scaffold
            .appBar (m/AppBar
                     .title (m/Text "MGL Rich Text Editor")
                     .elevation 0)
            .body (mongol-editor-view)
            ;; (m/Text "Hello, World!"))
            )))))